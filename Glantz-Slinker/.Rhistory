cortisol.range <- s[6] - s[1]
cortisol.iqr <- s[5] - s[2]
s # Summary
cortisol.iqr[[1]] # IQR
cortisol.range[[1]] # Range
depression$group <- factor(depression$group,levels = c(1,2,3), labels = c("Healthy","Non-melancholic","Melancholic"))
table(depression$group)
healthy <- subset(depression$cortisol, depression$group == "Healthy")
non.melancholic <- subset(depression$cortisol, depression$group == "Non-melancholic")
melancholic <- subset(depression$cortisol, depression$group == "Melancholic")
h.mean <- mean(healthy)
h.sd <- sd(healthy)
nm.mean <- mean(non.melancholic)
nm.sd <- sd(non.melancholic)
m.mean <- mean(melancholic)
m.sd <- sd(melancholic)
h.mean # Healthy mean
h.sd # Healthy standard deviation
nm.mean # Non-melancholic mean
nm.sd # Non-melancholic standard deviation
m.mean # Melancholic mean
m.sd # Melancholic standard deviation
par(mar = c(4,4,1,1), cex = 0.7)
boxplot(depression$cortisol ~ depression$group,las = 1, ylab = expression(paste("Blood Cortisol Level / (",mu,"g/dl)")))
depression.anova <- summary(aov(depression$cortisol ~ depression$group))
depression.anova
TukeyHSD(aov(depression$cortisol ~ depression$group))
pairwise.t.test(depression$cortisol,depression$group, p.adjust = "bonferroni")
